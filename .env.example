# LuminaWeb Environment Configuration

# API Keys (Required)
VITE_SCRAPINGBEE_API_KEY=your_scrapingbee_api_key_here
VITE_GROQ_API_KEY=your_groq_api_key_here

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration (Optional)
VITE_APP_NAME=LuminaWeb
VITE_APP_DESCRIPTION=Website Optimization Tool
VITE_APP_VERSION=1.0.0

# API Configuration (Optional)
VITE_API_TIMEOUT=30000
VITE_MAX_CONCURRENT_REQUESTS=5
VITE_ENABLE_DEBUG=false

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Feature Flags (Optional)
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_SCREENSHOTS=true
VITE_ENABLE_BULK_ANALYSIS=false
VITE_ENABLE_SCHEDULED_REPORTS=false